#!nsh
#
# PX4FMU startup script.

# Start playing the startup tune
tone_alarm start


echo "LSM303D validation firmware"
lsm303d start
rgbled start
rgbled rgb 0 50 0
if lsm303d guardian
then
	echo "Guardian exited without error. (Impossible!)"
else
	echo "LSM303D error!"
	# Play SOS
	tone_alarm error
	rgbled rgb 50 0 0
fi
